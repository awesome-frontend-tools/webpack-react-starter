{
  "name": "webpack-react-starter",
  "version": "0.0.1",
  "description": "Template for your frontend webpack project",
  "homepage": "https://github.com/awesome-frontend-tools/webpack-react-starter#readme",
  "scripts": {
    "start": "webpack serve --env mode=development",
    "start:analyze": "webpack serve --env mode=development addons=bundleanalyzer",
    "build": "webpack --env mode=production",
    "test:unit": "npx jest",
    "coverage": "npx jest --coverage",
    "prettier": "npx prettier --write **/*.{ts,tsx,json}",
    "lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix"
  },
  "author": "Kalachev Vlad",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-transform-runtime": "^7.22.4",
    "@babel/plugin-transform-typescript": "^7.22.3",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/circular-dependency-plugin": "^5.0.5",
    "@types/core-js": "^2.5.2",
    "@types/enzyme": "^3.10.13",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@types/webpack": "^5.28.1",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-manifest-plugin": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "dotenv": "^16.1.3",
